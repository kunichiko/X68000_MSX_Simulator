	beq	1f
	sub.b	#1,d1			* HCが立っている場合はさらに1を引く(F5,F3の評価のため)
1:	move.b	d1,d2			* 結果のbit3を取り出す
	andi.b	#%00001000,d2
	or.b	d2,d6			* F3をセット
	andi.b	#%00000010,d1
	lsl.b	#4,d1
	or.b	d1,d6			* F5をセット
	subq.w	#1,hl(a3)
	subq.w	#1,bc(a3)
	beq	@f
	ori.b	#%00000100,d6		* PVをセット
@@:	rts


	.dc.w	21			* サイクル数
	.dc.w	2-1			* 命令語長-1
CPDR:	bsr	CPD
	cmpi.w	#0,bc(a3)
