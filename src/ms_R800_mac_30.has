refresh_PC_if_needed:
	tst.w	ms_cpu_needs_refresh_PC
	beq	1f
	move.w	#0,ms_cpu_needs_refresh_PC
	swap	d7
	lea	ms_cpu_mem_ptr_array,a4
	movea.l	(a4,d7.w*4),a4
	swap	d7
1:	rts


