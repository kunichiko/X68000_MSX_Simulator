	andi.w	#$0f,d1
	move.b	(a0,d1.w),(a1)+

	rol.w	#4,d0
	move.w	d0,d1
	andi.w	#$0f,d1
	move.b	(a0,d1.w),(a1)+

	; IX
	move.w	ix(a3),d0
	lea.l	cpu_pc_msg_ix,a1

	rol.w	#4,d0
	move.w	d0,d1
	andi.w	#$0f,d1
	move.b	(a0,d1.w),(a1)+

	rol.w	#4,d0
	move.w	d0,d1
	andi.w	#$0f,d1
	move.b	(a0,d1.w),(a1)+

