	move.b	$80d,d0			* F6    : bit0
	and.b	$80e,d0			* SHIFT : bit0
	andi.b	#%00000001,d0		* F6とSHIFTの両方が押されているか？
	move.b	cpu_pc_f6_flg,d1
	lsl.b	#1,d1
	or.b	d0,d1			* b1:前回、b0:今回
	move.b	d1,cpu_pc_f6_flg
