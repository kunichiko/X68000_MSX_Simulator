	move.w	d0,d1
	andi.w	#$0f,d1
	move.b	(a0,d1.w),(a1)+

	rol.w	#4,d0
	move.w	d0,d1
	andi.w	#$0f,d1
	move.b	(a0,d1.w),(a1)+


	pea.l	cpu_pc_msg
	DOS	__PRINT
	lea.l	4(sp),sp

;	move.w	#$d,d1
;	IOCS	__BITSNS
