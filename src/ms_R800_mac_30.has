PC_page_inc	macro
		swap	d7
		addq.w	#1,d7
		andi.w	#PAGENUMMASK,d7
		lea	ms_cpu_mem_ptr_array,a4
		movea.l	(a4,d7.w*4),a4
		swap	d7
		endm

PC_page_dec	macro
		swap	d7
		subq.w	#1,d7
		andi.w	#PAGENUMMASK,d7
		lea	ms_cpu_mem_ptr_array,a4
		movea.l	(a4,d7.w*4),a4
		swap	d7
		endm

PC_page_set	macro			* JP命令等で PCが変わった時の処理
		swap	d7		* 上位 16bitに移動
		lsr.l	#8,d7		* PCの上位 3bit(Page番号)が上位ワードに残るようにする
		lsr.l	#8-PAGENUMBIT,d7
		lsr.w	#PAGENUMBIT,d7	* 下位16bitのみをシフト
		; この時点で以下のようになっている
		;  31     16 15 8 7  0
		; | PC page |  PC(8k) |  d7
		swap	d7
		lea	ms_cpu_mem_ptr_array,a4
		movea.l	(a4,d7.w*4),a4
		swap	d7
		endm

write_mem_8	macro
		jsr	ms_write_mem_8
		jsr	refresh_PC_if_needed
		endm

write_mem_16	macro
		jsr	ms_write_mem_16
		jsr	refresh_PC_if_needed
		endm

	.text

